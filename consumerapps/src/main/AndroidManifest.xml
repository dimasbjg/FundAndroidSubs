<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.consumerapps">

    <uses-permission
        android:name="com.example.fundandroidsubs.READ_DATABASE"/>
    <uses-permission
        android:name="com.example.fundandroidsubs.WRITE_DATABASE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FundAndroidSubs">
        <activity android:name=".ui.MainActivity" />
        <activity android:name=".ui.DetailUserActivity" />
        <activity android:name=".ui.FavoritesListActivity" />
        <activity android:name=".ui.SettingActivity" />
        <activity android:name=".ui.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <queries>
        <package android:name="com.example.fundandroidsubs" />
    </queries>

</manifest>